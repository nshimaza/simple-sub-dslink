{
  "name": "SimpleSubDSLink",
  "version": "0.0.1",
  "description": "Simple traffic generator for performance testing on IOT-DSA",
  "license": "Cisco Sample Code License",
  "author": {
    "name": "Naoto Shimazaki",
    "email": "nshimaza@cisco.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nshimaza/simple-sub-dslink"
  },
  "bugs": {
    "url": "https://github.com/nshimaza/simple-sub-dslink/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "SimpleSubDSLink"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "com.example.DsaGen$"
    }
  }
}
